#include <stdio.h>

int main () {
    
    int number1, number2, number3;
    
    scanf( "%d", &number1);
    
   if (number1 < 100) {
       printf("less then 100");
   } 
   else if (number1 == 100) {
       printf("Perfect!");
   }
   else {
       printf("Number is more then 100");
   }
    
    return 0;
}
